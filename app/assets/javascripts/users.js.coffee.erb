$('.user-snapshot').each (index, element) ->
  snapshot = $(this)
  update_snapshot = ->
    url = "/users/#{snapshot.data('user-id')}/snapshot.json"
    $.get url, (data) ->
      snapshot.empty()
      snapshot.html("<img src='#{data.snapshot_uri}' />")

  ten_seconds = 10000
  setInterval(update_snapshot, ten_seconds)
